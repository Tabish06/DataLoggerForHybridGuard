<p id="notice"><%= notice %></p>

<h1>Log Hybrid Guards</h1>

<table>
  <thead>
    <tr>
      <th>App name</th>
      <th>Working</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @log_hybrid_guards.each do |log_hybrid_guard| %>
      <tr>
        <td><%= log_hybrid_guard.app_name %></td>
        <td><%= log_hybrid_guard.working %></td>
        <td><%= log_hybrid_guard.comments %></td>
        <td><%= link_to 'Show', log_hybrid_guard %></td>
        <td><%= link_to 'Edit', edit_log_hybrid_guard_path(log_hybrid_guard) %></td>
        <td><%= link_to 'Destroy', log_hybrid_guard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Log Hybrid Guard', new_log_hybrid_guard_path %>
